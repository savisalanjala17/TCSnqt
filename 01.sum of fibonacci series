import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        if (n <= 0) {
            return;
        }

        int a = 0;
        int b = 1;
        int sum = 0;

        if (n == 1) {
            System.out.println("Fibonacci Series: " +a);
            System.out.println("Sum : " + a);
            return;
        }

        System.out.print("Fibonacci Series: " + a + " " + b + " ");
        sum = a + b;
        n -= 2;

        while (n > 0) {
            int c = a + b;
            System.out.print(c + " ");
            sum += c;
            a = b;
            b = c;
            n--;
        }

        System.out.println();
        System.out.println("Sum : " +sum);
    }
}
